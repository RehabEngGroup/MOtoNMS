<!--   AJC Computation: Mid Point
       Configuration file to indicate the Required Markers Name according to each Markers Protocol
	   Specify MarkerNames in the same order of MarkerFullNames    
--> 
<Method>
    <Name>AJCMidPoint</Name>
    <Input>
		<MarkerFullNames>
			<Marker>Left First Point</Marker>
            <Marker>Right First Point</Marker>
            <Marker>Left Second Point</Marker>
            <Marker>Right Second Point</Marker>
		</MarkerFullNames>
	</Input>
    <MarkersDefinition>
       <Protocol>
	      <Name>UWA-Fullbody</Name>
		  <MarkerNames>
			 <Marker>LLMAL</Marker>
			 <Marker>RLMAL</Marker>
			 <Marker>LMMAL</Marker>
			 <Marker>RMMAL</Marker>
		  </MarkerNames>
	    </Protocol>
        <Protocol>
          <Name>UNIPD_CASTforOpenSim</Name>  
	      <MarkerNames>
       	      <Marker>LLM</Marker>
              <Marker>RLM</Marker>
              <Marker>LMM</Marker>
              <Marker>RMM</Marker>
          </MarkerNames>			
        </Protocol>
		<Protocol>
          <Name>GU-10pointsCluster</Name>  
	      <MarkerNames>
			 <Marker>LLMAL</Marker>
			 <Marker>RLMAL</Marker>
			 <Marker>LMMAL</Marker>
			 <Marker>RMMAL</Marker>
          </MarkerNames>			
        </Protocol>	
        <Protocol>
          <Name>UMG-OpenSim</Name>  
	      <MarkerNames>
       	      <Marker>LLM</Marker>
              <Marker>RLM</Marker>
              <Marker>LMM</Marker>
              <Marker>RMM</Marker>
          </MarkerNames>			
        </Protocol>		
    </MarkersDefinition>  
</Method>  

 